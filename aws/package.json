{
  "name": "aws",
  "private": true,
  "version": "1.0.0-55",
  "description": "Example serverless functions using the Serverless-framework",
  "main": "src/handlers.js",
  "scripts": {
    "deploy": "serverless deploy -v"
  },
  "author": "Marco LÃ¼thy",
  "license": "MIT",
  "dependencies": {
    "chrome-aws-lambda": "^2.0.0",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.7.0",
    "googleapis": "^34.0.0",
    "puppeteer-core": "^2.0.0"
  },
  "devDependencies": {
    "serverless": "1.24.1",
    "serverless-webpack": "4.0.0",
    "webpack": "3.8.1"
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "6.10"
          },
          "include": [
            "es7.object.values",
            "es7.object.entries"
          ]
        }
      ],
      "stage-3"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-object-entries"
    ]
  }
}
